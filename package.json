        "module-resolver",
        {
          "root": [
            "./src"
          ],
          "alias": {
            "Components": "./src/components"
          }
        }
      ],
      [
        "inline-react-svg",
        {
          "svgo": {
            "plugins": [
              {
                "removeAttrs": {
                  "attrs": "(data-name)"
                }
              },
              {
                "cleanupIDs": false
              }
            ]
          }
        }
      ]
    ],
    "presets": [
      "@babel/preset-react",
      [
        "@babel/preset-env",
        {

        }
      ]
    ],
